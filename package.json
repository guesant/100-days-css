{
  "name": "100dayspostcss",
  "version": "1.0.0",
  "description": "100 days css challenge with postcss",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/index.html src/**/index.pug --no-cache",
    "build": "parcel build src/index.html src/**/index.pug",
    "lint": "stylelint src/**/*.css --fix",
    "update-config": "babel-node ./script/generate-range",
    "test": "exit 0"
  },
  "husky": {
    "hooks": {
      "post-commit": "npm run update-config && git add config.json && git commit -m 'chore: update config' --no-verify"
    }
  },
  "keywords": [
    "css",
    "postcss",
    "PostCSS"
  ],
  "author": "Jasmine Xie",
  "license": "ISC",
  "dependencies": {
    "element-ui": "^2.9.1",
    "parcel-bundler": "^1.12.3",
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3"
  },
  "devDependencies": {
    "@babel/node": "^7.4.5",
    "@vue/component-compiler-utils": "^3.0.0",
    "autoprefixer": "latest",
    "husky": "latest",
    "postcss-calc": "^7.0.1",
    "postcss-custom-properties": "^8.0.10",
    "postcss-for": "^2.1.1",
    "postcss-modules": "^1.4.1",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "postcss-simple-vars": "^5.0.2",
    "pug": "^2.0.3",
    "sass": "^1.20.3",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "vue-template-compiler": "^2.6.10"
  }
}
